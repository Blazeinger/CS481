func main()
{
  # correct calls for while/do while
  var int x = 0
  while(0 < 4)
  {
    x++
  }

  x = 0
  do
  {
    x++
  }
  while(x < 4)

  # incorrect calls for while due to not having a boolean value after while
  x = 12
  while(x)
  {
    x++
  }

  x = 0
  do
  {
    x++
  }
  while(x)
}